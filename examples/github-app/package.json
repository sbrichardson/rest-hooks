{
  "name": "github-app",
  "version": "0.1.8",
  "description": "github-app - A rest hooks example",
  "scripts": {
    "lint": "eslint src --ext .ts,.tsx",
    "format": "npm run lint --fix",
    "test:type": "tsc",
    "start": "webpack serve --mode=development",
    "start:prod": "serve dist",
    "build:browser": "webpack --mode=production",
    "build:analyze": "webpack --mode=production --env analyze",
    "build:profile": "webpack --mode=production --env profile",
    "test:pkg": "webpack --env check=nobuild"
  },
  "keywords": [
    "anansi"
  ],
  "main": "src/index.ts",
  "types": "src/index.ts",
  "private": true,
  "license": "Apache-2.0",
  "devDependencies": {
    "@anansi/babel-preset": "2.8.2",
    "@anansi/browserslist-config": "1.3.3",
    "@anansi/eslint-plugin": "0.12.8",
    "@anansi/webpack-config": "11.1.6",
    "@babel/core": "7.16.12",
    "@linaria/babel-preset": "3.0.0-beta.15",
    "@linaria/core": "3.0.0-beta.15",
    "@linaria/react": "3.0.0-beta.15",
    "@linaria/shaker": "3.0.0-beta.15",
    "@types/parse-link-header": "^1.0.0",
    "@types/react-dom": "17.0.11",
    "@types/react-router-dom": "^5.1.8",
    "react-refresh": "^0.11.0",
    "serve": "13.0.2",
    "webpack": "5.68.0",
    "webpack-cli": "4.8.0",
    "webpack-dev-server": "4.7.4"
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.4",
    "@rest-hooks/rest": "^3.0.1",
    "antd": "^4.16.13",
    "parse-link-header": "^2.0.0",
    "react-error-boundary": "^3.1.3",
    "react-markdown": "^7.0.1",
    "react-router-dom": "^5.2.1",
    "rest-hooks": "^6.2.0-beta.1"
  }
}
